   "source": [
    "import pandas as pd\n",
    "\n",
    "\n",
    "def pandas_rolling(series, window, freq=1, method=\"mean\"):\n",
    "    series_list = []\n",
    "    for i in range(freq):\n",
    "        _series = series.iloc[i::freq].rolling(window).agg(method)\n",
    "        series_list.append(_series)\n",
    "    return pd.concat(series_list, axis=0).sort_index()"
   ]
  }
